# 1. Create project dir and set up backend
git clone
cd video_codec

# 2. create py venv
python -m venv venv
source venv/bin/activate

# 3. install backed dependencies
pip install flask python-magic numpy

# 4. set up react frontend
cd frontend
npm install

# 6. install req frontend dependencies
npm install lucide-react @radix-react-alert-dialog tailwindcss postcss autoprefixer @tailwindcss/forms

# 7. build
npm build

# 8. run app
./run.sh
